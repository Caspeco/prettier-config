{
	"name": "@caspeco/prettier-config",
	"version": "1.0.2",
	"description": "Caspeco prettier config",
	"main": "index.js",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/Caspeco/prettier-config"
	},
	"bugs": {
		"url": "https://github.com/caspeco/prettier-config/issues"
	},
	"engines": {
		"node": ">=24",
		"npm": ">=11.5.1"
	},
	"keywords": [
		"prettier",
		"prettier-config"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint": "eslint . --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --report-unused-disable-directives --fix",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"test": "vitest",
		"ci-test": "CI=true vitest"
	},
	"prettier": "./index.js",
	"peerDependencies": {
		"prettier": "^3.0.0"
	},
	"devDependencies": {
		"@caspeco/eslint-config": "4.0.5",
		"prettier": "^3.2.5",
		"vite": "^7.0.0",
		"vitest": "^3.0.2"
	},
	"dependencies": {}
}
